		, l.inf_fax
		, l.inf_phone
		, l.answers
		, l.sold
                                     ,l.status
		, c.[name]
		, cf.value_fields
  FROM [staging].[staging].[join_leads] l
	INNER JOIN [staging].[values_custom_fields] cf ON l.id = cf.cid
		AND l.[dbid] = cf.[dbid]
	INNER JOIN [staging].[custom_fields] c ON cf.[ref_custom_fields] = c.id
  -- where l.id = 7686
) AS SourceTable PIVOT(MAX(value_fields) FOR name IN( ipaddress,
                                                         sex,
                                                         created,
                                                         dob,
                                                         [source],
														 prize,
														 affliateid,
														 title,
														 id)) AS PivotTable;</property>
